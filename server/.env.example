# Server Configuration
PORT=5000
NODE_ENV=development

# Database Connection
MONGODB_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/quickkart?retryWrites=true&w=majority

# Session Security
SESSION_SECRET=your_super_secret_session_key

# Frontend URLs (CORS)
# Production URLs for Vercel deployments
CLIENT_URL=https://your-client-app.vercel.app
ADMIN_URL=https://your-admin-app.vercel.app

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
